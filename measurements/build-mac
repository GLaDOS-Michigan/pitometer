# Project build file for mac
#!/bin/bash

LOCK_DIR='/Users/zhang/Dropbox/UMich2020sp/pitometer/measurements/toylock-go'
export GOPATH=$LOCK_DIR

echo "Starting build"

go build "$LOCK_DIR/src/Main.go"

export GOPATH='/Users/zhang/go'
export PATH=$PATH:$(go env GOPATH)/bin
export GOPATH=$(go env GOPATH)

echo "Build complete"
